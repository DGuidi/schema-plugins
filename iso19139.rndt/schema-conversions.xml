    <converter name="rndt2dc"
               service="xml_csw-record"
               nsUri="http://www.openarchives.org/OAI/2.0/"
               schemaLocation="http://www.openarchives.org/OAI/2.0/oai_dc.xsd"
               xslt="xml_rndt2dc.xsl"/>
</conversions>
